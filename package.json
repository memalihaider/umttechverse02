{
  "name": "reg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:fix": "npm audit fix",
    "security:check": "npm run security:audit && npm run security:dependencies",
    "security:dependencies": "npx depcheck --ignores='@types/*,@vercel/*'",
    "security:headers": "npx check-my-headers https://localhost:3000",
    "security:scan": "npx npm-check-updates --target minor",
    "security:lockfile": "npm audit --audit-level=high --json | npx jq '.vulnerabilities | keys[]'",
    "precommit": "npm run lint && npm run security:audit",
    "migrate:add-team-member-cnic": "node scripts/add_team_member_cnic.js",
    "migrate:add-team-member-cnic:apply": "node scripts/add_team_member_cnic.js --apply",
    "verify:team-member-cnic": "node scripts/verify_team_member_cnic.js"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "@supabase/supabase-js": "^2.81.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.3",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.553.0",
    "next": "16.0.2",
    "nodemailer": "^7.0.10",
    "pdfkit": "^0.13.0",
  "pdf-lib": "^1.17.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "resend": "^6.4.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pdfkit": "^0.12.3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
